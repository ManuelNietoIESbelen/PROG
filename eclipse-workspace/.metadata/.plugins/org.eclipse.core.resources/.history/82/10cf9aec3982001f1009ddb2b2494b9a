package r01ejercicios;
import java.util.*;
public class r01e29 {
  
 public static void main(String[] args) {
    int num;
  
   Scanner sc = new Scanner(System.in);
   
   System.out.println("Introduce un numero");
   num=sc.nextInt();
   
   boolean comprobar = 20>= num && num < 30 ;
   
   
     
   
   
   System.out.println(comprobar);
}

  

}
